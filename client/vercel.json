{
  "rewrites": [
    {
      "source": "/auth/kakao/callback/:code*",
      "destination": "https://smartchart.vercel.app/auth/kakao/callback/:code*"
    },

    {
      "source": "/oauth2/authorization/kakao/:code*",
      "destination": "https://smartchart.vercel.app/oauth2/authorization/kakao/:code*"
    },
    {
      "source": "/code",
      "destination": "http://13.125.227.145:8080/code"
    },

    {
      "source": "/doctor/:path*",
      "destination": "http://13.125.227.145:8080/doctor/:path*"
    },
    {
      "source": "/patient/:path*",
      "destination": "http://13.125.227.145:8080/patient/:path*"
    },
    {
      "source": "/ws/chat/:path*",
      "destination": "http://13.125.227.145:8080/ws/chat/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
