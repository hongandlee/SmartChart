{
  "build": {
    "env": {
      "REACT_APP_API_URL": "@api-url"
    }
  },
  "rewrites": [
    {
      "source": "/doctor/:path*/",
      "destination": "https://$REACT_APP_API_URL/doctor/:path*/"
    },
    {
      "source": "/patient/:path*/",
      "destination": "https://$REACT_APP_API_URL/doctor/:path*/"
    }
  ]
}
